@model InventoryManager.Application.Elements.ElementDto

@{
    ViewData["Title"] = "New Element";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="row">
        <h1>Elemento</h1>
        <form asp-action="DeleteElement" class="col s12">

            <div class="row">
                <div class="input-field col s6">
                    <input  id="nameElement" type="text" class="validate">
                    <label >Nombre</label>
                </div>

                <div class="input-field col s6">
                    <button class="btn waves-effect waves-light" type="submit" name="sacar">
                        Sacar
                        <i class="material-icons right">delete</i>
                    </button>
                </div>
            </div>

        </form>
    </div>


</div>



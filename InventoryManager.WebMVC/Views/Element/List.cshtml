@using InventoryManager.Application.Elements



@{
    ViewData["Title"] = "Elementos";
}

<div class="card">
    <div class="row">
        <h1>Listado de elementos</h1>
        <a class="btn-floating btn-large waves-effect waves-light green right" href="@Url.Action("New")"><i class="material-icons">add</i></a>

        <table>
            <thead>
                <tr>
                    <th>Identificador</th>
                    <th>Nombre</th>
                    <th>Fecha de entrada</th>
                    <th>Fecha de caducidad</th>
                    <th>Tipo</th>

                </tr>
            </thead>

            <tbody>
                <tr>

                    @foreach (var element in Model)
                    {
                        <td>
                            element.Id
                        </td>
                        <td>
                            element.name
                        </td>
                        <td>
                            element.EntryDate
                        </td>
                        <td>
                            element.ExpirationDate
                        </td>
                        <td>
                            element.Type
                        </td>
                    }

                </tr>
            </tbody>
        </table>


    </div>
</div>

